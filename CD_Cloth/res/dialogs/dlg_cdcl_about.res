		{
			COLUMNS 1;
			NAME CDCL_SERIAL;
			BORDERSIZE 12,6,12,6;
			BORDERSTYLE BORDER_GROUP_IN;
			SCALE_H;

			EDITTEXT IDC_CDCL_SERIALNUMBER { SCALE_H; }
		}
	}

	DLGGROUP { OK; }
}
